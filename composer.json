{
    "name": "usmonaliyev/env-auth",
    "version": "1.0.0",
    "description": "...",
    "keywords": [
        "usmonaliyev",
        "config"
    ],
    "homepage": "https://github.com/usmonaliyev/env-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "usmonaliyev99",
            "email": "t.usmonaliyev99@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Usmonaliyev\\EnvAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Usmonaliyev\\EnvAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Usmonaliyev\\EnvAuth\\EnvAuthServiceProvider"
            ],
            "aliases": {
                "EnvAuth": "Usmonaliyev\\EnvAuth\\Facades\\EnvAuth"
            }
        }
    }
}
